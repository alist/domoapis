{
  "name": "domo-io-server",
  "version": "1.0.0",
  "scripts": {
    "start": "node app.js",
    "test": "node_modules/.bin/mocha --reporter spec --timeout 15000",
    "test2": "node_modules/.bin/mocha --reporter spec --grep '^(?!LOAD)' --timeout 15000",
    "load": "mocha --reporter spec --grep '^LOAD' --timeout 15000",
    "http": "mocha --reporter spec --grep '^HTTP' --timeout 15000",
    "push": "git push heroku master; git push github master"
  },
  "engines": {
    "node": "0.10.x"
  },
  "dependencies": {
    "coffee-script": "1.2.0",
    "supervisor": "0.5.x",
    "mongoose": "~3.6.14",
    "express": "~3.3.1",
    "request": "2.9.x",
    "sendgrid": "0.2.7",
    "nodemailer": "~0.4.4",
    "passport": "~0.1.17",
    "passport-local": "~0.1.6",
    "bcrypt": "~0.7.5",
    "crypto": "~0.0.3",
    "node-uuid": "~1.4.1",
    "validator": "~1.1.1",
    "lodash": "~1.3.1",
    "connect-flash": "~0.1.1",
    "jade": "~0.34.1",
    "async": "~0.2.9",
    "twilio": "~1.1.4",
    "token": "0.0.0",
    "apn": "~1.4.1",
    "node-gcm": "~0.9.6"
  },
  "devDependencies": {
    "chai": "1.6.0",
    "mocha": "1.9.0",
    "supertest-chai": "0.0.7"
  }
}
